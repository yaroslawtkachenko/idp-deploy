{
  "name": "idp",
  "version": "1.0.0",
  "license": "MIT",
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%"
  ],
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config ./webpack/webpack.config.prod.js",
    "dev": "webpack serve --config ./webpack/webpack.config.dev.js --hot",
    "lint:js": "npx eslint ./src --ext .js --ext .jsx --ext .tsx --ext .ts --format table",
    "lint:js:fix": "yarn run lint:js --fix"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^5.2.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-i18next": "^5.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-jsx-max-len": "^1.0.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.2.4",
    "mini-css-extract-plugin": "^1.4.1",
    "node-sass": "^5.0.0",
    "prettier": "^2.0.5",
    "sass-loader": "^11.0.1",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(js|jsx|ts|tsx)": [
      "prettier --write",
      "yarn run lint:js:fix",
      "git add"
    ]
  }
}
